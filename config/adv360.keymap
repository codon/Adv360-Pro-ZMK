        mod {
            display-name = "Mod";
            bindings = <
&none           &bt BT_SEL 0  &bt BT_SEL 1  &bt BT_SEL 2  &bt BT_SEL 3  &bt BT_SEL 4  &none                                                                       &trans        &bt BT_SEL 0  &bt BT_SEL 1  &bt BT_SEL 2  &bt BT_SEL 3  &bt BT_SEL 4  &none
&none           &none         &none         &none         &none         &none         &bootloader                                                                 &bootloader   &none         &none         &none         &none         &none         &none
&studio_unlock  &none         &none         &none         &none         &none         &stp STP_BAT  &bt BT_CLR       &bt BT_CLR      &bt BT_CLR  &bt BT_CLR       &stp STP_BAT  &none         &none         &none         &none         &none         &none
&none           &none         &none         &none         &macro_ver    &none                                        &none           &none                                      &none         &none         &none         &none         &none         &none
&none           &none         &none         &bl BL_INC    &bl BL_DEC                  &bl BL_TOG    &rgb_ug RGB_TOG  &none           &none       &rgb_ug RGB_TOG  &bl BL_TOG                  &bl BL_DEC    &bl BL_INC    &none         &none         &none
            >;
        };

        Qwerty {
            bindings = <
&kp EQUAL  &kp N1         &kp N2     &kp N3  &kp N4  &kp N5  &trans                                      &trans  &kp N6  &kp N7  &kp N8     &kp N9   &kp N0    &kp MINUS
&trans     &kp Q          &kp W      &kp E   &kp R   &kp T   &trans                                      &trans  &kp Y   &kp U   &kp I      &kp O    &kp P     &kp BSLH
&trans     &kp A          &kp S      &kp D   &kp F   &kp G   &trans  &trans  &trans      &trans  &trans  &trans  &kp H   &kp J   &kp K      &kp L    &kp SEMI  &kp APOS
&trans     &kp Z          &kp X      &kp C   &kp V   &kp B                   &trans      &trans                  &kp N   &kp M   &kp COMMA  &kp DOT  &kp FSLH  &trans
&trans     &kp BACKSLASH  &kp GRAVE  &trans  &trans          &trans  &trans  &trans      &trans  &trans  &trans          &trans  &trans     &trans   &trans    &trans
            >;
        };

        Gaming_Left {
            bindings = <
&trans  &trans    &trans   &trans  &trans  &trans  &trans                                               &trans  &trans  &trans  &trans     &trans   &trans     &trans
&trans  &kp T     &kp Q    &kp W   &kp E   &kp R   &trans                                               &trans  &kp U   &kp I   &kp O      &kp P    &kp MINUS  &trans
&trans  &kp G     &kp A    &kp S   &kp D   &kp F   &trans        &trans     &trans      &trans  &trans  &trans  &kp H   &kp J   &kp K      &kp L    &kp SEMI   &trans
&trans  &kp B     &kp Z    &kp X   &kp C   &kp V                            &trans      &trans                  &kp N   &kp M   &kp COMMA  &kp DOT  &kp FSLH   &trans
&trans  &kp BSPC  &kp DEL  &trans  &trans          &htp 2 SPACE  &kp ENTER  &trans      &trans  &trans  &trans          &trans  &trans     &trans   &trans     &trans
            >;
